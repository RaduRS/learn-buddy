<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="200" fill="#87CEEB"/>
  
  <!-- Ground -->
  <rect x="0" y="150" width="300" height="50" fill="#90EE90"/>
  
  <!-- Pink flower -->
  <g transform="translate(150, 100)">
    <!-- Stem -->
    <rect x="-2" y="20" width="4" height="40" fill="#228B22"/>
    
    <!-- Leaves -->
    <ellipse cx="-15" cy="35" rx="8" ry="4" fill="#228B22" transform="rotate(-30)"/>
    <ellipse cx="15" cy="35" rx="8" ry="4" fill="#228B22" transform="rotate(30)"/>
    
    <!-- Flower petals -->
    <ellipse cx="0" cy="-15" rx="8" ry="15" fill="#FF69B4"/>
    <ellipse cx="12" cy="-8" rx="8" ry="15" fill="#FF69B4" transform="rotate(45)"/>
    <ellipse cx="15" cy="5" rx="8" ry="15" fill="#FF69B4" transform="rotate(90)"/>
    <ellipse cx="8" cy="15" rx="8" ry="15" fill="#FF69B4" transform="rotate(135)"/>
    <ellipse cx="-8" cy="15" rx="8" ry="15" fill="#FF69B4" transform="rotate(-135)"/>
    <ellipse cx="-15" cy="5" rx="8" ry="15" fill="#FF69B4" transform="rotate(-90)"/>
    <ellipse cx="-12" cy="-8" rx="8" ry="15" fill="#FF69B4" transform="rotate(-45)"/>
    
    <!-- Flower center -->
    <circle cx="0" cy="0" r="6" fill="#FFD700"/>
    <circle cx="0" cy="0" r="3" fill="#FF8C00"/>
  </g>
  
  <!-- Some grass -->
  <g stroke="#228B22" stroke-width="2">
    <line x1="50" y1="150" x2="52" y2="145"/>
    <line x1="80" y1="150" x2="82" y2="143"/>
    <line x1="220" y1="150" x2="222" y2="144"/>
    <line x1="250" y1="150" x2="252" y2="146"/>
  </g>
  
  <!-- Butterfly -->
  <g transform="translate(200, 80)">
    <ellipse cx="-3" cy="-2" rx="4" ry="6" fill="#FF1493"/>
    <ellipse cx="3" cy="-2" rx="4" ry="6" fill="#FF1493"/>
    <ellipse cx="-2" cy="3" rx="3" ry="4" fill="#FF69B4"/>
    <ellipse cx="2" cy="3" rx="3" ry="4" fill="#FF69B4"/>
    <line x1="0" y1="-5" x2="0" y2="5" stroke="black" stroke-width="1"/>
  </g>
  
  <!-- Sun -->
  <circle cx="250" cy="50" r="15" fill="#FFD700"/>
</svg>