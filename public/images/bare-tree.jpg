<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Sky background -->
  <rect width="300" height="200" fill="#87CEEB"/>
  
  <!-- Ground -->
  <rect x="0" y="150" width="300" height="50" fill="#8B7355"/>
  
  <!-- Bare tree -->
  <g transform="translate(150, 150)">
    <!-- Trunk -->
    <rect x="-8" y="-60" width="16" height="60" fill="#8B4513"/>
    
    <!-- Main branches -->
    <line x1="0" y1="-60" x2="-25" y2="-80" stroke="#8B4513" stroke-width="4"/>
    <line x1="0" y1="-60" x2="25" y2="-80" stroke="#8B4513" stroke-width="4"/>
    <line x1="0" y1="-45" x2="-20" y2="-65" stroke="#8B4513" stroke-width="3"/>
    <line x1="0" y1="-45" x2="20" y2="-65" stroke="#8B4513" stroke-width="3"/>
    
    <!-- Smaller branches -->
    <line x1="-25" y1="-80" x2="-35" y2="-90" stroke="#8B4513" stroke-width="2"/>
    <line x1="-25" y1="-80" x2="-30" y2="-75" stroke="#8B4513" stroke-width="2"/>
    <line x1="25" y1="-80" x2="35" y2="-90" stroke="#8B4513" stroke-width="2"/>
    <line x1="25" y1="-80" x2="30" y2="-75" stroke="#8B4513" stroke-width="2"/>
    
    <!-- Tiny twigs -->
    <line x1="-35" y1="-90" x2="-40" y2="-95" stroke="#8B4513" stroke-width="1"/>
    <line x1="-35" y1="-90" x2="-32" y2="-93" stroke="#8B4513" stroke-width="1"/>
    <line x1="35" y1="-90" x2="40" y2="-95" stroke="#8B4513" stroke-width="1"/>
    <line x1="35" y1="-90" x2="32" y2="-93" stroke="#8B4513" stroke-width="1"/>
  </g>
  
  <!-- Some grass -->
  <g stroke="#228B22" stroke-width="2">
    <line x1="50" y1="150" x2="52" y2="140"/>
    <line x1="80" y1="150" x2="82" y2="145"/>
    <line x1="220" y1="150" x2="222" y2="142"/>
    <line x1="250" y1="150" x2="252" y2="144"/>
  </g>
</svg>